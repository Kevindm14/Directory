<div>
 <h1>Users</h1>
<table class="form-control">
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Address</th>
    <th>Email</th>
    <th>City</th>
    <th>State</th>
    <th>Cellphone</th>
    <th>Phone</th>
    <th>Company</th>
    <th colspan="4"></th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.address %></td>
      <td><%= user.email %></td>
      <td><%= user.city %></td>
      <td><%= user.state %></td>
      <td><%= user.cellphone %></td>
      <td><%= user.phone %></td>
      <td><%= user.company %></td>
      <td>
        <%= link_to 'Show', user %>
        <%= link_to 'Edit user', edit_user_path(user) %>
        <%= link_to 'Delete user', user,
                    method: :delete,
                    data: { confirm: 'Â¿Are you sure?'} %>
      </td>
    </tr>
  <% end %>
</table>
</div>
